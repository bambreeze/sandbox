#!/usr/bin/expect -f

set timeout 60

set remote_server 192.168.1.10
set my_user_id "root"
set my_password "123456"
set my_command "ls"

puts "hello, Expect"
spawn ssh $my_user_id@$remote_server
expect "password:" {send "$my_password\r"}
expect "%" {send "exit\r"}
expect "closed"
sleep 3
return
